    "embla-carousel-react": "^8.0.0-rc03",
    "next": "^13.3.4",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.4.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
